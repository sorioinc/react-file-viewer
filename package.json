{
  "name": "@sorioinc/react-file-viewer",
  "version": "1.2.5",
  "description": "Extendable file viewer for web",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sorioinc/react-file-viewer.git"
  },
  "scripts": {
    "build": "webpack build --config ./webpack.config.js --stats --progress",
    "start": "webpack serve --config ./webpack.config.dev.js",
    "lint": "node_modules/.bin/eslint ./src/**/*.jsx",
    "test": "node_modules/.bin/jest --env=jsdom --watch",
    "jest": "node_modules/.bin/jest --env=jsdom",
    "tag-and-publish": "node ./scripts/publish.mjs"
  },
  "author": "PlanGrid <opensource@plangrid.com>",
  "contributors": [
    {
      "name": "Alexei Schiopu",
      "email": "alexxschiopu@gmail.com",
      "url": "http://schiopu.me"
    },
    {
      "name": "Aaron Norby",
      "email": "aaron.norby@gmail.com"
    },
    {
      "name": "Viktoriya Savkina",
      "email": "viktoriya.savkina@gmail.com"
    },
    {
      "name": "Oscar Soriano"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plangrid/react-file-viewer/issues"
  },
  "homepage": "https://github.com/plangrid/react-file-viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "chalk": "^5.0.0",
    "css-loader": "^6.5.1",
    "dart-sass": "^1.25.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.5",
    "postcss-loader": "^6.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "comma-separated-values": "^3.6.4",
    "mammoth": "1.4.19",
    "pdfjs-dist": "2.10.377",
    "prop-types": "^15.7.2",
    "react-data-grid": "^6.1.0",
    "react-visibility-sensor": "^5.1.1",
    "sass": "^1.45.0",
    "three": "0.135.0",
    "xlsx": "^0.17.4"
  },
  "peerDependencies": {
    "react": "^16.14 || ^17.0",
    "react-dom": "^16.14 || ^17.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|scripts|example_files)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "moduleNameMapper": {
      "pdfjs-dist/webpack": "pdfjs-dist"
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "webpack": "webpack.config.js"
      }
    }
  }
}
